<odoo>
  <record id='estate_tag_tree' model='ir.ui.view'>
    <field name='name'>tag.tree</field>
    <field name='model'>estate_property_tag</field>
    <field name='arch' type='xml'>
      <tree string='Propery Tag' editable='bottom'>
        <field name='name'/>
      </tree>
    </field>
  </record>

  <record id='estate_tag_action' model='ir.actions.act_window'>
    <field name='name'>tag.action</field>
    <field name='res_model'>estate_property_tag</field>
    <field name='view_mode'>tree, form</field>
  </record>
</odoo>
